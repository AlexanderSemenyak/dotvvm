<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	<AssemblyTitle>DotVVM.Tracing.MiniProfiler.AspNetCore</AssemblyTitle>
    <VersionPrefix>2.0.0-pre08</VersionPrefix>
    <TargetFrameworks>netstandard2.0</TargetFrameworks>
    <NoWarn>$(NoWarn);CS1591</NoWarn>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <AssemblyOriginatorKeyFile>dotvvmwizard.snk</AssemblyOriginatorKeyFile>
    <SignAssembly>true</SignAssembly>
    <PublicSign Condition=" '$(OS)' != 'Windows_NT' ">true</PublicSign>
    <PackageId>DotVVM.Tracing.MiniProfiler.AspNetCore</PackageId>
    <PackageVersion>2.0.0-pre08</PackageVersion>
    <Authors>RIGANTI</Authors>
    <Description>MiniProfiler Tracing module for ASP.NET Core and DotVVM</Description>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageTags>dotvvm;asp.net;mvvm;owin</PackageTags>
    <PackageIconUrl>https://dotvvm.com/Content/images/icons/icon-blue-64x64.png</PackageIconUrl>
    <PackageLicenseUrl>https://github.com/riganti/dotvvm/blob/master/LICENSE</PackageLicenseUrl>
    <PreserveCompilationContext>true</PreserveCompilationContext>  
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="DotVVM" Version="2.0.0-pre08" />
    <PackageReference Include="DotVVM.AspNetCore" Version="2.0.0-pre08" />
    <PackageReference Include="MiniProfiler.AspNetCore" Version="4.0.0-alpha8-165" />
  </ItemGroup>

</Project>
